{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc build active file",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${file}",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}",
                "-I${NANOCBOR_INCLUDE}",
                "-L${NANOCBOR_BUILD}",
                "-I${ZCBOR_INCLUDE}",
                "${ZCBOR_SOURCE}/zcbor_common.c",
                "${ZCBOR_SOURCE}/zcbor_decode.c",
                "${ZCBOR_SOURCE}/zcbor_encode.c",
                "${ZCBOR_SOURCE}/zcbor_print.c",
                "../src/hashmap.c",
                "../src/sid.c",
                "../src/coreconfManipulation.c",
                "../src/serialization.c",
                "../src/zcborSerialization.c",
                "../src/coreconfTypes.c",
                "-lm",
                "-lnanocbor"
            ],
            "options": {
                "cwd": "${fileDirname}",
                "env": {
                    "NANOCBOR_INCLUDE": "${env:NANOCBOR_INCLUDE}",
                    "NANOCBOR_BUILD": "${env:NANOCBOR_BUILD}",
                    "ZCBOR_INCLUDE": "${env:ZCBOR_INCLUDE}",
                    "ZCBOR_SOURCE": "${env:ZCBOR_SOURCE}",
                    "PATH": "${env:PATH}"
                }
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}