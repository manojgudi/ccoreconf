{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc build active file",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${file}",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}",
                "-I${NANOCBOR_INCLUDE}",
                "-L${NANOCBOR_BUILD}",
                "-I${LIBJANSSON_INCLUDE}",
                "-L${LIBJANSSON_BUILD}",
                "../src/hashmap.c",
                "../src/sid.c",
                "../src/fileOperations.c",
                "../src/coreconfManipulation.c",
                "../src/serialization.c",
                "../src/coreconfTypes.c",
                "-ljansson",
                "-lm",
                "-lnanocbor"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        },
        {
            "type": "cppbuild",
            "label": "C/C++: gcc-11 build active file",
            "command": "/usr/bin/gcc-11",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${file}",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}",
                "-I${NANOCBOR_INCLUDE}",
                "-L${NANOCBOR_BUILD}",
                "-I${LIBJANSSON_INCLUDE}",
                "-L${LIBJANSSON_BUILD}",
                "../src/hashmap.c",
                "../src/sid.c",
                "../src/fileOperations.c",
                "../src/coreconfManipulation.c",
                "../src/serialization.c",
                "../src/coreconfTypes.c",
                "-ljansson",
                "-lm",
                "-lnanocbor"
            ],
            "options": {
                "cwd": "${fileDirname}",
                "env": {
                    "NANOCBOR_INCLUDE": "${env:NANOCBOR_INCLUDE}",
                    "NANOCBOR_BUILD": "${env:NANOCBOR_BUILD}",
                    "LIBJANSSON_BUILD": "${env:LIBJANSSON_BUILD}",
                    "LIBJANSSON_INCLUDE": "${env:LIBJANSSON_INCLUDE}"
                }
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": "build",
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}